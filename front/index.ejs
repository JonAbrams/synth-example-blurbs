<!DOCTYPE html>
<html>
  <head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'>
    <meta name='viewport' content='width=device-width, initial-scale=1.0'>
    <title><%= appName %></title>

    <% cssFiles.forEach(function (cssFile) { %>
      <link rel='stylesheet' href='<%= cssFile %>'>
    <% }) %>

    <!-- Preloaded Data  -->
    <script>
      var preloadedData = <%- data %>;
    </script>

    <% jsFiles.forEach(function (jsFile) { %>
      <script src='<%= jsFile %>'></script>
    <% }) %>
  </head>
  <body ng-app='blurbs'>
    <a href="https://github.com/JonAbrams/synth-example-blurbs"><img style="z-index: 1;position: absolute; top: 0; right: 0; border: 0;" src="https://camo.githubusercontent.com/e7bbb0521b397edbd5fe43e7f760759336b5e05f/68747470733a2f2f73332e616d617a6f6e6177732e636f6d2f6769746875622f726962626f6e732f666f726b6d655f72696768745f677265656e5f3030373230302e706e67" alt="Fork me on GitHub" data-canonical-src="https://s3.amazonaws.com/github/ribbons/forkme_right_green_007200.png"></a>

    <nav class='navbar navbar-default'>
      <div class='container'>
        <div class='navbar-header'>
          <a class='navbar-brand' ng-href="/blurbs">Blurbs</a>
        </div>
      </div>
    </nav>

    <div class='container'>
      <div ng-view></div>
    </div>

    <% if (preloadHTML) { %>
      <script type="text/ng-template" id="<%= preloadHTMLPath %>">
        <%- preloadHTML %>
      </script>
    <% } %>
  </body>
</html>
